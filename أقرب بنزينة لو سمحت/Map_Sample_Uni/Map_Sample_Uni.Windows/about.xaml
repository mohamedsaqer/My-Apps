        <!--app version-->
        <TextBlock Text="V 1.0.0.0" TextWrapping="Wrap" FontSize="14" TextAlignment="Left" Foreground="Black"  HorizontalAlignment="Center" />
        <!--app description-->
        <TextBlock TextWrapping="Wrap" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Right" Foreground="Black" TextAlignment="left" FlowDirection="RightToLeft" Text="هذا التطبيق يساعدك على الوصول لأقرب بنزينة منك بسرعة و سهولة عن طريق تحديد مكانك و اماكن البنزينات المحيطة بك و رسم الطريق لأقرب واحدة. كما يساعدك على معرفة الخدمات التى تقدمها البنزينة. و كمان لوعايز بنزينة معينة هيعرفك توصلها ازاى على الخريطة و يعرفك عنوانه بالكامل" Margin="15" Height="200"/>
        <TextBlock Foreground="#FF0095DA" TextWrapping="Wrap" Text="Developed By :" FontSize="20" FontWeight="ExtraLight"/>
        <!--developer name-->
        <TextBlock Foreground="Blue" TextWrapping="Wrap" Text=" MOHAMED EL_SAQER " FontSize="25" FontWeight="Bold" Tapped="TextBlock_Tapped"/>
        <TextBlock TextWrapping="Wrap" Text="Assembly line 5-1 Program - Trainee | Egypt AppFactory"  FontSize="15" Foreground="Black"/>
        <!--university name-->
